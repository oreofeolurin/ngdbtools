{
  "name": "dbtools",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "rimraf dist",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:core": "ng build core --configuration production",
    "build:common": "ng build common --configuration production",
    "build:dynamic-form": "ng build dynamic-form --configuration production",
    "build:dynamic-table": "ng build dynamic-table --configuration production",
    "build:libs": "yarn build:core && yarn build:common && yarn build:dynamic-form",
    "package:core": "npm run build:core && cd dist/core && npm pack",
    "package:common": "npm run build:common && cd dist/common && npm pack",
    "package:dynamic-form": "npm run build:dynamic-form && cd dist/dynamic-form && npm pack",
    "package:dynamic-table": "npm run build:dynamic-table && cd dist/dynamic-table && npm pack",
    "package": "npm run package:core && npm run package:common && npm run package:dynamic-form"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/cdk": "^15.1.4",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/material": "^15.1.4",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "bootstrap": "^5.2.3",
    "class-validator": "^0.14.0",
    "gsap": "^3.11.4",
    "ng2-currency-mask": "^13.0.3",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.5",
    "@angular/cli": "~15.1.5",
    "@angular/compiler-cli": "^15.1.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.1.0",
    "typescript": "~4.9.4"
  }
}